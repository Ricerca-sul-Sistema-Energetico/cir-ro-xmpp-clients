# environment variables for running in debug mode
# mqtt
mqtt_host =  0.0.0.0  			# the MQTT broker IP 
mqtt_port =  1883			# the MQTT broker port  	
mqtt_username = broker_username		# the MQTT broker username (provided by broker admins) 
mqtt_password = broker_password		# the MQTT broker password (provided by broker admins) 
mqtt_qos = 0				# the Quality of Service for MQTT connection 
