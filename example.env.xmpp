# environment variables for running in debug mode
# xmpp
node = "cir"						# the node of this client jabber id
domain = "server_domain"  				# the domain of this client jabber id
password =  server_password  				# the XMPP server password (provided by server admins) 
server_host = server_host   				# the XMPP server host 
server_port = 5222 					# the XMPP server port
